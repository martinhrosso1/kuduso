{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "version": "1.1.0",
  "description": "Minimal debug bindings - single number in/out",
  "inputs": [
    {
      "gh_param": "input_value",
      "json_path": "$.value",
      "type": "number",
      "description": "Maps input number to Grasshopper parameter"
    }
  ],
  "outputs": [
    {
      "gh_param": "output_value",
      "json_path": "$.result",
      "type": "number",
      "description": "Maps Grasshopper output to result"
    }
  ]
}

