# AppServer Configuration

# Server port
PORT=8080

# Use real Rhino.Compute (false = mock mode)
USE_COMPUTE=false

# Rhino.Compute URL (for future use)
COMPUTE_URL=http://localhost:9000

# Rhino.Compute API Key (for future use)
COMPUTE_API_KEY=

# Contracts directory (relative to appserver root)
CONTRACTS_DIR=../../contracts

# Logging level (debug, info, warn, error)
LOG_LEVEL=info

# Request timeout (milliseconds)
TIMEOUT_MS=240000
